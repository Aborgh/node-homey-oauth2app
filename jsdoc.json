{
  "plugins": [
    "jsdoc-ts-utils",
    "plugins/markdown"
  ],
  "source": {
    "include": [
      "./README.md",
      "./lib",
      "./index.js"
    ]
  },
  "opts": {
    "simpleAnalyticsHost": "sa.athom.com",
    "mainpagetitle": "Homey OAuth2",
    "destination": "./jsdoc",
    "template": "./node_modules/@athombv/jsdoc-template"
  },
  "tsUtils": {
    "typeScriptUtilityTypes": false
  }
}